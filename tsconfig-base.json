{
    "compilerOptions": {
        "declaration": true,
        "target": "es2020",
        "module": "es2020",
        "strict": true,
        "noUnusedLocals": false,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "noEmitOnError": true,
        "skipLibCheck": true,
        "composite": true,
        "moduleResolution": "node",
    }
}